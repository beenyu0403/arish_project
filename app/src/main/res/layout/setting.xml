<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">


    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp">

        <!--배경-->

        <ImageView
            android:id="@+id/private_setting"
            android:layout_width="400dp"
            android:layout_height="230dp"
            android:onClick="mOnClick"
            android:src="@drawable/edittext_img"/>

        <ImageView
            android:id="@+id/notice"
            android:layout_width="400dp"
            android:layout_height="230dp"
            android:layout_marginTop="80dp"
            android:onClick="mOnClick"
            android:src="@drawable/edittext_img"/>

        <ImageView
            android:id="@+id/notification"
            android:layout_width="400dp"
            android:layout_height="230dp"
            android:onClick="mOnClick"
            android:layout_marginTop="160dp"
            android:src="@drawable/edittext_img"/>

        <ImageView
            android:id="@+id/QnA"
            android:layout_width="400dp"
            android:layout_height="230dp"
            android:onClick="mOnClick"
            android:layout_marginTop="240dp"
            android:src="@drawable/edittext_img"/>


        <!--화살표-->

        <ImageView
            android:id="@+id/private_ck"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_gravity="right"
            android:layout_marginRight="50dp"
            android:onClick="mOnClick"
            android:layout_marginTop="100dp"
            android:src="@drawable/icon_right_24px" />
        <ImageView
            android:id="@+id/notice_ck"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_gravity="right"
            android:layout_marginRight="50dp"
            android:onClick="mOnClick"
            android:layout_marginTop="180dp"
            android:src="@drawable/icon_right_24px" />
        <ImageView
            android:id="@+id/notification_ck"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_gravity="right"
            android:onClick="mOnClick"
            android:layout_marginRight="50dp"
            android:layout_marginTop="260dp"
            android:src="@drawable/icon_right_24px" />
        <ImageView
            android:id="@+id/QnA_ck"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_gravity="right"
            android:onClick="mOnClick"
            android:layout_marginRight="50dp"
            android:layout_marginTop="340dp"
            android:src="@drawable/icon_right_24px" />


        <!--text_img-->

        <ImageView
            android:id="@+id/private_text"
            android:layout_width="100dp"
            android:layout_height="23dp"
            android:onClick="mOnClick"
            android:layout_marginTop="97dp"
            android:layout_marginLeft="70dp"
            android:src="@drawable/private_text"/>
        <ImageView
            android:id="@+id/notice_text"
            android:layout_width="100dp"
            android:layout_height="23dp"
            android:onClick="mOnClick"
            android:layout_marginTop="177dp"
            android:layout_marginLeft="63dp"
            android:src="@drawable/notice_text"/>
        <ImageView
            android:id="@+id/notification_text"
            android:layout_width="100dp"
            android:layout_height="23dp"
            android:onClick="mOnClick"
            android:layout_marginTop="257dp"
            android:layout_marginLeft="64dp"
            android:src="@drawable/notification_text"/>
        <ImageView
            android:id="@+id/QnA_text"
            android:layout_width="100dp"
            android:layout_height="23dp"
            android:onClick="mOnClick"
            android:layout_marginTop="337dp"
            android:layout_marginLeft="80dp"
            android:src="@drawable/qna_text"/>
    </FrameLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="60dp">

        <ImageView
            android:id="@+id/img_backnn3"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:onClick="mOnClick"
            android:src="@drawable/iconblackleft" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:text="설정"
            android:textColor="@color/black"
            android:textSize="15dp"
            android:textStyle="bold" />
    </LinearLayout>


</FrameLayout>